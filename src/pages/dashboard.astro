---
import PageLayout from '~/layouts/PageLayout.astro';
import LogoutButton from '~/components/authentication/LogoutButton';
---

<PageLayout metadata={{ title: 'Entrar al portal de socios' }}>
  <div class="h-[calc(100vh-var(--header-height)-var(--footer-height))]">
    <section class="relative w-full h-full flex flex-col items-center justify-center mt-20">
      <h1 class="text-2xl font-bold mb-6">Dashboard de socios</h1>
      <LogoutButton client:load />
    </section>
  </div>
</PageLayout>
